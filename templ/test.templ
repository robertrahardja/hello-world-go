
package templ



templ TestTempl() {
    <!DOCTYPE html>
	<html lang="en">
	<head>
		<meta charset="UTF-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
		<title>HTMX Counter</title>
		<script src="https://unpkg.com/htmx.org@1.9.6"></script>
	</head>
	<body>
		<h1>HTMX Counter</h1>
		<button hx-post="/test/increment" hx-target="#counter">
			Increment
		</button>
    <h1>
    Counter: <span id="counter">0</span>
    </h1>
	</body>
	</html>
  }


